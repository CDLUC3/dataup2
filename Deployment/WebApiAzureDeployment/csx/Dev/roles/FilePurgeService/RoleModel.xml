<?xml version="1.0" encoding="utf-8"?>
<RoleModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="FilePurgeService" version="2.2.6492.2" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <Startup>
    <Task commandLine="DiagnosticsAgent.exe" taskType="background" relativePath="plugins\Diagnostics">
      <Environment>
        <Variable name="RdModuleName" value="Diagnostics" />
        <Variable name="WaRoleType" value="WorkerRole" />
      </Environment>
    </Task>
    <Task commandLine="DiagnosticsAgent.exe /blockStartup" relativePath="plugins\Diagnostics">
      <Environment>
        <Variable name="RdModuleName" value="Diagnostics" />
        <Variable name="WaRoleType" value="WorkerRole" />
      </Environment>
    </Task>
  </Startup>
  <Runtime>
    <EntryPoint>
      <NetFxEntryPoint assemblyName="Microsoft.Research.DataOnboarding.FilePurgeService.dll" targetFrameworkVersion="v4.0" />
    </EntryPoint>
  </Runtime>
  <ConfigurationSettings>
    <Setting name="ConnectionString" nameSpace="Microsoft.WindowsAzure.Plugins.Diagnostics" />
  </ConfigurationSettings>
  <Properties>
    <Property name="TargetFrameworkVersion" value="v4.5" />
    <Property name="RoleType" value="Worker" />
    <Property name="RoleName" value="FilePurgeService" />
    <Property name="ProjectName" value="FilePurgeService" />
    <Property name="Configuration" value="Dev" />
    <Property name="Platform" value="AnyCPU" />
    <Property name="RoleTargetFramework" value="v4.5" />
    <Property name="OutputDir" value="C:\DataOnboarding\Deployment\WebApiAzureDeployment\obj\Dev\FilePurgeService\" />
    <Property name="EntryPoint" value="Microsoft.Research.DataOnboarding.FilePurgeService.dll" />
    <Property name="SourceDir" value="" />
    <Property name="SitePhysicalPath" value="" />
    <Property name="TargetRuntimeVersion" value="v4.0" />
  </Properties>
</RoleModel>